<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.PrimaryController">
   <children>
      <Label text="Primary View" />
      <Label text="Username" />
      <TextField fx:id="inputUser"/>
      <Label text="Password" />
      <PasswordField fx:id="inputPass"/>
      <Button id="buttonConnect" fx:id="buttonConnect" mnemonicParsing="false" text="Connect" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
